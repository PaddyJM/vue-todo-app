{
  "name": "vue-todo-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "postinstall": "cd api/ && npm install && cd ../",
    "dev": "vite --mode development",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build:development": "vite build --mode development",
    "build:production": "vite build --mode production",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "deploy:development": "npm run build:development && cd api/ && npx cdk bootstrap && DEPLOYMENT_ENV=dev npx cdk deploy --require-approval=never && cd ../",
    "deploy:production": "npm run build:production && cd api/ && npx cdk bootstrap && DEPLOYMENT_ENV=prod npx cdk deploy --require-approval=never && cd ../"
  },
  "dependencies": {
    "@auth0/auth0-vue": "^2.2.0",
    "@aws-sdk/client-dynamodb": "^3.347.0",
    "vue": "^3.3.2",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@iconify/vue": "^4.1.1",
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.19",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "typescript": "~5.0.4",
    "uid": "^2.0.2",
    "vite": "^4.3.5",
    "vue-tsc": "^1.6.4"
  }
}
